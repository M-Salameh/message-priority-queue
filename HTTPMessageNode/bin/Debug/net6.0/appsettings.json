{
  "spring": {
    "application": {
      "name": "http-message-node"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "database": {
    "conn": "localhost:27017"
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "true",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "AllowedHosts": "*"
  },

  "OpenTelemetry": {
    "Tracing": {
      "Exporter": {
        "Jaeger": {
          "AgentHost": "localhost",
          "AgentPort": 6831
        }
      }
    }
  }
}
