{
  "spring": {
    "application": {
      "name": "SchedulerNode"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.Hosting.Lifetime": "Error"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "false",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    }
  },
  "instance": {},
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },

  "OpenTelemetry": {
    "ServiceName": "Scheduler-1",
    "Tracing": {
      "Exporter": {
        "Jaeger": {
          "AgentHost": "localhost",
          "AgentPort": 6831
        }
      }
    }
  },
  "RedisInfo": {
    "Connection": "localhost:63790",
    "Providors": "SYR,MTN",
    "Levels": "6"
  },

  "MessageStorage": {
    "Mongo": "mongodb://127.0.0.1:27017",
    "DBName": "scheduled-messages",
    "collection": "messages"
  }
}